@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix library: <http://purl.org/library/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix uwext: <http://faculty.washington.edu/tgis/ld/brumfield/uwDataset/extensions#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix vra: <http://purl.org/vra/> .
@prefix wgs84: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:AdministrativeAreaShape a sh:NodeShape ;
    sh:targetClass vra:AdministrativeArea .

ex:CityShape a sh:NodeShape ;
    sh:targetClass vra:City .

ex:EventShape a sh:NodeShape ;
    sh:targetClass vra:Event .

ex:GeoCoordShape a sh:NodeShape ;
    sh:targetClass schema:GeoCoord .

ex:PlaceShape a sh:NodeShape ;
    sh:targetClass vra:Place .

ex:WorkShape a sh:NodeShape ;
    sh:targetClass vra:Work .

ex:componentOfShape a sh:PropertyShape ;
    sh:path vra:componentOf .

ex:creatorShape a sh:PropertyShape ;
    sh:path vra:creator .

ex:descriptionShape a sh:PropertyShape ;
    sh:path vra:description .

ex:endDateShape a sh:PropertyShape ;
    sh:path vra:endDate .

ex:formerSiteShape a sh:PropertyShape ;
    sh:path vra:formerSite .

ex:geoShape a sh:PropertyShape ;
    sh:path schema:geo .

ex:isReferencedByShape a sh:PropertyShape ;
    sh:path dcterms:isReferencedBy .

ex:latShape a sh:PropertyShape ;
    sh:path wgs84:lat .

ex:latitudeShape a sh:PropertyShape ;
    sh:path schema:latitude .

ex:longShape a sh:PropertyShape ;
    sh:path wgs84:long .

ex:longitudeShape a sh:PropertyShape ;
    sh:path schema:longitude .

ex:nameShape a sh:PropertyShape ;
    sh:path vra:name .

ex:placeOfSiteShape a sh:PropertyShape ;
    sh:path vra:placeOfSite .

ex:sameAsShape a sh:PropertyShape ;
    sh:path <http://www.w3.org/2002/07/owl#sameAs> .

ex:startDateShape a sh:PropertyShape ;
    sh:path vra:startDate .

ex:streetAddressShape a sh:PropertyShape ;
    sh:path uwext:streetAddress .

ex:typeShape a sh:PropertyShape ;
    sh:path dcterms:type,
        rdf:type .

ex:wasAlteredShape a sh:PropertyShape ;
    sh:path vra:wasAltered .

ex:wasCreatedShape a sh:PropertyShape ;
    sh:path vra:wasCreated .

ex:wasDestroyedShape a sh:PropertyShape ;
    sh:path vra:wasDestroyed .

ex:wasRestoredShape a sh:PropertyShape ;
    sh:path vra:wasRestored .

ex:worktypeShape a sh:PropertyShape ;
    sh:path uwext:worktype .


